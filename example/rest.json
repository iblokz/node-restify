{
	"users": {
		"model": "User",
		"collectionCtrls": {
			"get": ["me"]
		},
		"documentCtrls": {
			"get": ["articles"]
		},
		"schema": {
			"name": "String",
		    "email": "String",
		    "password": "String",
		    "date_created": { "type": "Date", "default": "Date.now" }
		}
	},
	"articles": {
		"model": "Article",
		"schema": {
			"title" : "String",
			"body" : "String",
			"createdBy" : { "type": "ObjectId", "ref":"User", "required": false}
		}
	}
}